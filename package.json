{
  "name": "yeps-bodyparser",
  "version": "1.1.0",
  "description": "YEPS Parse request bodies",
  "main": "index.js",
  "scripts": {
    "lint": "npm-run-all --parallel lint:**",
    "lint:js": "eslint index.js tests",
    "test": "npm-run-all --parallel test:**",
    "test:security": "nsp check",
    "test:code": "mocha tests --recursive",
    "test:coverage": "istanbul cover _mocha -- tests --recursive",
    "report": "cat ./coverage/lcov.info | coveralls",
    "clear": "rimraf coverage",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evheniy/yeps-bodyparser.git"
  },
  "keywords": [
    "promise",
    "body",
    "http",
    "server",
    "rest",
    "fast",
    "async",
    "await",
    "https",
    "ssl",
    "easy",
    "parser",
    "json",
    "text",
    "file"
  ],
  "author": "Evheniy Bystrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evheniy/yeps-bodyparser/issues"
  },
  "homepage": "https://github.com/evheniy/yeps-bodyparser#readme",
  "files": [
    "index.js",
    "lib"
  ],
  "engines": {
    "node": ">=7.6.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "debug": "^3.0.1",
    "eslint": "^4.7.2",
    "eslint-config-eslint": "^4.0.0",
    "husky": "^0.14.3",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.1.1",
    "nsp": "^2.8.0",
    "promise-pause-timeout": "^1.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^3.3.0"
  },
  "dependencies": {
    "yeps": "^1.0.0",
    "yeps-body": "^1.1.0"
  }
}
